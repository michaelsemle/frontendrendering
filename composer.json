{
	"name": "trueprogramming/frontendrendering",
	"type": "typo3-cms-extension",
	"description": "Provides basic typoscript configuration for templating",
	"license": ["GPL-2.0-or-later"],
	"keywords": [
		"TYPO3 CMS",
		"project",
		"frontend",
		"typoscript",
		"configuration"
	],
	"require": {
		"typo3/cms-backend": "^11 || ^12",
		"typo3/cms-core": "^11 || ^12",
		"typo3/cms-fluid-styled-content": "^11 || ^12"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "frontendrendering"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	}
}
